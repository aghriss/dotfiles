PURPLE='\033[01;35m'
CYAN='\033[01;36m'
WHITE='\033[01;37m'
BOLD='\033[1m'
UNDERLINE='\033[4m'
BD=${RED}
HR=${GREEN}
SAY=${YELLOW}

echo -e "                     ${BD}__          -„${NONE}"
echo -e "                      ${BD}*-„¯¯*^~-„„_\*-„${NONE}"
