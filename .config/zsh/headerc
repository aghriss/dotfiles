echo -e "            ${BD}„-^*${NONE}${HR}::::::::::::::::::::::::::${NONE}${BD}/${NONE}"
echo -e "          ${BD}„-*${NONE}${HR}:::::::::::::::::${NONE}${BD}___„„„-„-^*¯${NONE}"
echo -e "  ${BD}\*^~--„„-*${NONE}${HR}:::::::::${NONE}${BD}_„„„-~~^^¯${NONE}           \ "
echo -e "   ${BD}" "\\" "${NONE}${HR}::::::::::::${NONE}${BD}-^*${NONE}        _„„-*„„-*¯¯**^-„"
echo -e " ${BD}„-^*'${NONE}${HR}::::::::::${NONE}${BD}¯*^-${NONE}   „-^**¯   *-„   o |"
echo -e " ${BD}¯*^~„${NONE}${HR}:::::::::${NONE}${BD}-^*¯${NONE}    |    o   |¯*^--~^**^-„"
echo -e "     ${BD}|${NONE}${HR}::::::::::::${NONE}${BD}*-„${NONE}    \„_._„„„-     _„-'„"
echo -e "      ${BD}" "\\" "${NONE}${HR}:::::::::::::${NONE}${BD}*-„${NONE}   ¯¯      *¯    *~-„„_"
echo -e "       ${BD}*-„${NONE}${HR}::::„-~-„„:::${NONE}${BD}|${NONE}                      ¯) "
echo -e "          ${BD}*~-„„_„/-~„${NONE}${HR}..${HR}${BD}*'${NONE}          __„„„„-~^**"
